<include>
  <X-PRE-PROCESS cmd="set" data="sound_prefix=$${sounds_dir}/en/us/callie"/>

  <X-PRE-PROCESS cmd="set" data="global_codec_prefs=OPUS,PCMU,PCMA"/>

  <X-PRE-PROCESS cmd="set" data="conference_energy_level=100"/>
  <X-PRE-PROCESS cmd="set" data="conference_jitterbuffer=60:300"/>

  <X-PRE-PROCESS cmd="set" data="external_sip_port=5060"/>
  <X-PRE-PROCESS cmd="set" data="external_rtp_ip=auto-nat"/>
  <X-PRE-PROCESS cmd="set" data="external_sip_ip=auto-nat"/>

  <!-- event socket password -->
  <X-PRE-PROCESS cmd="set" data="event_socket_password=ClueCon"/>

  <!-- https://freeswitch.org/confluence/display/FREESWITCH/Configuring+FreeSWITCH#ConfiguringFreeSWITCH-vars.xml -->
  <X-PRE-PROCESS cmd="include" data="passwords.xml"/>
</include>
