<include>
  <!-- Prompt set -->
  <X-PRE-PROCESS cmd="set" data="sound_prefix=$${sounds_dir}/en/us/callie"/>

  <!-- Codecs -->
  <X-PRE-PROCESS cmd="set" data="global_codec_prefs=OPUS,PCMU,PCMA"/>

  <!-- Conference quality params -->
  <X-PRE-PROCESS cmd="set" data="conference_energy_level=100"/>
  <X-PRE-PROCESS cmd="set" data="conference_jitterbuffer=60:120"/>

  <!-- Shared external IPv4 sip/rtp addresses -->
  <X-PRE-PROCESS cmd="set" data="external_sip_port=auto"/>
  <X-PRE-PROCESS cmd="set" data="external_rtp_ip=auto-nat"/>
  <X-PRE-PROCESS cmd="set" data="external_sip_ip=auto-nat"/>

  <!-- Connection params for browsers via websocket (ipv4/ipv6) -->
  <X-PRE-PROCESS cmd="set" data="browser_sip_port=auto"/>

  <!-- Connection params for browsers via websocket (ipv4) -->
  <X-PRE-PROCESS cmd="set" data="browser_local_sip_ip_v4=$${local_ip_v4}"/>
  <X-PRE-PROCESS cmd="set" data="browser_local_rtp_ip_v4=$${local_ip_v4}"/>
  <X-PRE-PROCESS cmd="set" data="browser_external_sip_ip=$${external_sip_ip}"/>
  <X-PRE-PROCESS cmd="set" data="browser_external_rtp_ip=$${external_rtp_ip}"/>
  <X-PRE-PROCESS cmd="set" data="browser_ws_binding_v4=:5066"/>
  <X-PRE-PROCESS cmd="set" data="browser_wss_binding_v4=:7443"/>

  <!-- Connection params for browsers via websocket (ipv6) -->
  <X-PRE-PROCESS cmd="set" data="browser_sip_ip_v6=$${local_ip_v6}"/>
  <X-PRE-PROCESS cmd="set" data="browser_rtp_ip_v6=$${local_ip_v6}"/>
  <X-PRE-PROCESS cmd="set" data="browser_ws_binding_v6=:5066"/>
  <X-PRE-PROCESS cmd="set" data="browser_wss_binding_v6=:7443"/>

  <!-- Connection params for bbb-webrtc-sfu via websocket -->
  <X-PRE-PROCESS cmd="set" data="sfu_sip_port=auto"/>
  <X-PRE-PROCESS cmd="set" data="sfu_sip_ip=$${local_ip_v4}"/>
  <X-PRE-PROCESS cmd="set" data="sfu_rtp_ip=$${local_ip_v4}"/>
  <X-PRE-PROCESS cmd="set" data="sfu_ws_binding=:5096"/>
  <X-PRE-PROCESS cmd="set" data="sfu_wss_binding=:9443"/>

  <!-- event socket password -->
  <X-PRE-PROCESS cmd="set" data="event_socket_password=ClueCon"/>

  <!-- https://freeswitch.org/confluence/display/FREESWITCH/Configuring+FreeSWITCH#ConfiguringFreeSWITCH-vars.xml -->
  <X-PRE-PROCESS cmd="include" data="passwords.xml"/>
</include>
