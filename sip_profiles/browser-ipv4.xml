<profile name="browser-ipv4">
  <settings>
    <param name="auth-calls" value="false"/>

    <param name="dialplan" value="XML"/>
    <param name="context" value="browser"/>

    <param name="codec-prefs" value="$${global_codec_prefs}"/>
    <param name="inbound-late-negotiation" value="true"/>

    <param name="apply-candidate-acl" value="ice"/>
    <param name="sip-ip" value="$${local_ip_v4}"/>
    <param name="rtp-ip" value="$${local_ip_v4}"/>
    <param name="ext-sip-ip" value="$${external_ip_v4}"/>
    <param name="ext-rtp-ip" value="$${external_ip_v4}"/>
    <param name="sip-port" value="$${browser_sip_port}"/>

    <!-- WebSockets for WebRTC signalling -->
    <param name="ws-binding"  value="$${browser_ws_binding_v4}"/>
    <param name="wss-binding"  value="$${browser_wss_binding_v4}"/>

    <!-- enable rtcp on every channel also can be done per leg basis with rtcp_audio_interval_msec variable set to passthru to pass it across a call -->
    <param name="rtcp-audio-interval-msec" value="5000"/>
    <param name="rtcp-video-interval-msec" value="5000"/>

    <!-- Necessary for transfer from echo-test to conference -->
    <param name="liberal-dtmf" value="true"/>

    <!-- Profile database (full in-memory) -->
    <param name="dbname" value="sqlite://memory://file:browser-ipv4?mode=memory&amp;cache=shared"/>
  </settings>
</profile>
